@page "/logout"
@inject HttpClient Http


<MudText>Logging out...</MudText>

@code {
    protected override async Task OnInitializedAsync()
    {
        try
        {
            var response = await Http.PostAsync("CoreService/logout", null);

        }
        catch (Exception ex)
        {
            Console.WriteLine($"Fehler beim Logout: {ex.Message}");
       
        }
    }
}